{
  "devDependencies": {
    "browser-sync": "^2.2.4",
    "browserify": "^9.0.3",
    "can": "^2.2.0-alpha.10",
    "jquery": "^2.1.3",
    "uglify-js": "^2.4.17"
  },
  "scripts": {
    "js": "browserify js/main.js -o js/app.js",
    "postjs": "uglifyjs js/app.js -o js/app.min.js --source-map js/app.min.js.map",
    "predev": "npm run js",
    "dev": "browser-sync start --server --files='index.html'"
  }
}
